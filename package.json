{
  "name": "restyle",
  "version": "2.0.0",
  "description": "The simplest way to add CSS to your React components.",
  "author": "Travis Arnold (http://souporserious.com)",
  "license": "MIT",
  "homepage": "https://www.restyle.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/souporserious/restyle.git"
  },
  "bugs": {
    "url": "https://github.com/souporserious/restyle/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/souporserious"
  },
  "keywords": [
    "react",
    "css",
    "css-in-js",
    "styled",
    "styling"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./css": {
      "types": "./dist/css.d.ts",
      "import": "./dist/css.js",
      "default": "./dist/css.js"
    },
    "./styled": {
      "types": "./dist/styled.d.ts",
      "import": "./dist/styled.js",
      "default": "./dist/styled.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js",
      "default": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "import": "./dist/jsx-dev-runtime.js",
      "default": "./dist/jsx-dev-runtime.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "npm run build -- --watch",
    "build:site": "npm run build --workspaces"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "typescript": "^5.4.5"
  },
  "workspaces": [
    ".",
    "site"
  ]
}
